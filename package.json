{
  "name": "rbsx",
  "version": "1.0.0",
  "description": "React, Bootstrap, SASS Boilerplate",
  "homepage": "https://rbsx.moquette.us",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moquette/rbsx"
  },
  "author": "Joaquin A. Moquette <me@moquette.us> (https://www.moquette.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moquette/rbsx/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint 'src/**/*.{js,jsx,json}'",
    "lint:fix": "eslint . --fix",
    "format": "yarn prettier --write --loglevel log .",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "predeploy": "yarn run build",
    "test": "react-scripts test",
    "prepare": "husky install",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "yarn prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "eslint-plugin-cypress": "^2.11.3",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.37.5",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.11.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
